    "chronoHistory": [{
      "date": 1870,
      "desc": ["Immaculate Conception Orphan Asylum was established by nuns affiliated with the Society of the Sacred Heart of Mary to educate young Native girls. Not long after the school's opening, it was compelled to enrol destitute non-Native children."],
      "ref": ["SJS-000012", "SJS-000017", "SJS-000023", "SJS-000153"]
    }, {
      "date": 1885,
      "desc": ["St. Joseph's (Fort William) Indian Residential School came under the charge of the Sisters of St. Joseph. "],
      "ref": ["SJS-000835", "SJS-000153"]
    }, {
      "date": 1895,
      "desc": ["On April 10, the orphanage building and Roman Catholic Church were destroyed by fire.", "The rebuilding of the school and church were financed by the Fort William Band. The new orphanage was completed in the fall."],
      "ref": ["SJS-000078", "SJS-000078a", "SJS-000080", "SJS-000153", "SJS-000081", "SJS-000094", "SJS-000091a", "SJS-000846"]
    }, {
      "date": 1909,
      "desc": ["St. Joseph's (Fort William) Indian Residential School relocated to the city of Fort William on January 8. Relocation was required after the school and land were expropriated by the Grand Trunk Pacific Railway."],
      "ref": ["SJS-000859", "SJS-000447a", "SJS- 000232", "SJS-000213"]
    }, {
      "date": 1968,
      "desc": ["As of September, St. Joseph's (Fort William) Indian Residential School was used only as a hostel."],
      "ref": ["SJS-000546", "SJS-000734"]
    }, {
      "date": 1970,
      "desc": ["St. Joseph's (Fort William) Indian Residential School closed on June 30."],
      "ref": ["SJS- 000567"]
    }],
